  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/setup-jest.ts"
    ],
    "testMatch": [
      "**/?(*.)+(jest).ts"
    ],
    "setupFiles": [
      "<rootDir>/jest.polyfills.js"
    ],
    "testEnvironmentOptions": {
      "customExportConditions": [
        ""
